tolerance 2.1
filetype pdb
output packmol.pdb
seed 378405
add_box_sides 

structure ./top/SED_anion_SED.pdb 
  number SED_na_SED
  inside box 0.0 0.0 0.0 SED_x_SED SED_y_SED SED_z_SED
end structure

structure ./top/SED_cation_SED.pdb 
  number SED_na_SED
  inside box 0.0 0.0 0.0 SED_x_SED SED_y_SED SED_z_SED
end structure
